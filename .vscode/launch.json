{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "attach",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/node_modules"],
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Mustache",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/mustache/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug webpack plugin",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/webpack-cli/dist/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug acorn ast tree",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/ast/acorn.js"
    }
  ]
}
